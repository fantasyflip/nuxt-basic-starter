{
  "apiVersion": "apps/v1",
  "kind": "Deployment",
  "metadata": {
    "name": "nuxt-basic-starter",
    "namespace": "nuxt-basic-starter"
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "nuxt-basic-starter"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "nuxt-basic-starter"
        }
      },
      "spec": {
        "imagePullSecrets": [
          {
            "name": "ghcr-secret"
          }
        ],
        "containers": [
          {
            "name": "nuxt-basic-starter",
            "image": "ghcr.io/fantasyflip/nuxt-basic-starter:main-19e7a38245815b602297d92a0ce56dbeab508797",
            "ports": [
              {
                "containerPort": 3000
              }
            ],
            "resources": {
              "requests": {
                "cpu": "500m",
                "memory": "750Mi"
              },
              "limits": {
                "cpu": "1000m",
                "memory": "1000Mi"
              }
            }
          }
        ]
      }
    }
  }
}
