    "eslint-plugin-prettier": "^5.5.1",
    "nuxt": "^4.0.2",
    "nuxt-og-image": "5.1.9",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.0.2",
    "yaml": "^2.8.0"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
